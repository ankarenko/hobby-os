{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Launch QEMU2",
      "type": "shell",
      //"command": "echo Hello",
      "command": "qemu-system-i386 -S -gdb tcp::1234 ${env:MYOS_QEMU_PARAMS} -boot d -daemonize -kernel isodir/boot/myos.bin || exit 0",
      "dependsOn": [
        "Build"
      ]
      //"isBackground": true
    },
    {
      "label": "Launch test QEMU",
      "type": "shell",
      //"command": "echo Hello",
      "command": "qemu-system-i386 -S -gdb tcp::1234 ${env:MYOS_QEMU_PARAMS} -daemonize -boot d -kernel isodir/boot/test.bin || exit 0",
      "dependsOn": [
        "Build-TEST"
      ]
      //"isBackground": true
    },
    {
      "label": "Build-TEST",
      "command": "sh",
      "args":["./make.sh", "test-build"]
    },
    {
      "label": "Build",
      "command": "sh",
      "args":["./make.sh", "build"]
    },
    {
      "label": "Kill QEMU",
      "type": "shell",
      "command": "kill -9 $(ps -ef | awk '/[q]emu-system-i386/{print $2}')",
    }
  ]
}